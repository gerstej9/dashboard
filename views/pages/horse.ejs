        <% userLiveTotalUsd = (userLiveTotal*nmrPrice)%>
        <p> Live Total USD: $<%=userLiveTotalUsd%></p>
      </div>

    </main>
    <footer>

    </footer>
    <script src= '/app.js'></script>
  </body>
</html>